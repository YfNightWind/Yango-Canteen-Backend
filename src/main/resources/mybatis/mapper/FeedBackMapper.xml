<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alexlin.yangocanteen.mapper.FeedBackMapper">
    <insert id="sendFeedBack" parameterType="FeedBack">
        insert into feedback(username, contact, feedback)
        values (#{username}, #{feedback}, #{contact})
    </insert>
</mapper>